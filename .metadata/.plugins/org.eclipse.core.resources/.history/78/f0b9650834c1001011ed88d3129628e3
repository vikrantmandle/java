package com.app.basic.streams.data;

import java.util.List;

import com.app.java.streams.prep.Product;

public class Product {
	String name; String category; double price;

	public Product(String name, String category, double price) {
		super();
		this.name = name;
		this.category = category;
		this.price = price;
	}

	public static List<Product> populateProducts(){
		List<Product> products = List.of(Product("","Laptop", 85000.0),
			     Product("","Smartphone", 50000.0),
			    Product("","Earbuds", 5000.0),
			    Product("","Smartwatch", 15000.0),
			    Product("","Monitor", 15000.0),
			    Product("","Mouse", 3000.0)
	}
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getCategory() {
		return category;
	}

	public void setCategory(String category) {
		this.category = category;
	}

	public double getPrice() {
		return price;
	}

	public void setPrice(double price) {
		this.price = price;
	}
	
	

}
